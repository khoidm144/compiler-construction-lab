program Exercise3;

var a : integer;
    myArray : array (.10.) of integer;
    count : integer;

BEGIN
    count := 0;
    n := readI;
    if n > 10 then call WriteC('E');

    for i := 0 to n-1 do  
        myArray(.i.) := readI;
    for i := 0 to n-1 do
        if myArray(.i.) < myArray(.0.) then count := count + 1;

    call writeI(count);
END.